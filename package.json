    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.1",
    "css-loader": "^0.28.7",
    "firebase": "^4.3.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "lodash": "^4.17.21",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^3.2.2",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "wallaby-webpack": "0.0.39",
    "webdriver-manager": "^12.0.6",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "zone.js": "^0.8.17"
  },
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "FIREBASE_PROJECT='truly-kryptic-dev' webpack-dev-server -d --host 0.0.0.0",
    "test": "FIREBASE_PROJECT='truly-kryptic-dev' karma start --single-run",
    "build": "webpack -p",
    "serve-prod": "http-server dist/ -a localhost -p 8080",
    "webd": "webdriver-manager start",
    "e2e": "protractor protractor.conf.js",
    "webd-docker": "docker run --net=host -e CHROMEDRIVER_URL_BASE='/wd/hub' -p 4444:4444 -d robcherry/docker-chromedriver:latest",
    "e2e-travis": "export FIREBASE_PROJECT='truly-kryptic-dev'; npm run webd-docker && npm run build; npm run serve-prod & npm run e2e",
    "deploy-dev": "export FIREBASE_PROJECT='truly-kryptic-dev'; rm dist/ -rf; npm run build; firebase deploy --project truly-kryptic-dev --token \"$FIREBASE_TOKEN\"",
    "deploy-prod": "export FIREBASE_PROJECT='truly-kryptic'; rm dist/ -rf; npm run build; firebase deploy --project truly-kryptic --token \"$FIREBASE_TOKEN\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/hb20007/truly-kryptic.git"
  },
  "author": "hb20007",
  "license": "ISC",
  "homepage": "https://bitbucket.org/hb20007/truly-kryptic#readme"
}
